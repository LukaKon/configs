{
   "brand" : "bhyve",
   "acpi" : "off",
   "autoboot" : "false",
   "bootargs" : "",
   "bootdisk" : {
      "blocksize" : "8K",
      "path" : "rpool/vm/freebsd",
      "size" : "30G",
      "sparse" : "false"
   },
   "vcpus": 2,
   "ram": "2G",
   "bootrom" : "BHYVE_RELEASE",
   "cdrom" : [
      "/iso/FreeBSD-14.0-RELEASE-amd64-dvd1.iso"
   ],
   "device" : [
      {
         "match" : "/dev/zvol/rdsk/rpool/freebsd"
      }
   ],
   "fs": [
      {
         "dir": "/coding",
         "special": "/coding",
         "type": "lofs",
         "options": ["rw", "nodevices"]
      },
      {
         "dir": "/config",
         "special": "/home/lk/configs",
         "type": "lofs",
         "options": ["ro", "nodevices"]
      }
   ],
   "fs-allowed" : "",
   "hostid" : "",
   "ip-type" : "exclusive",
   "limitpriv" : "",
   "net" : [
      {
         "global-nic": "e1000g0",
         "allowed-address" : "192.168.1.80/24",
         "defrouter" : "192.168.1.1",
         "physical" : "free0"
      }
   ],
   "pool" : "",
   "scheduling-class" : "",
   "vnc" : {
      "enabled" : "on"
   },
   "zonename" : "freebsd",
   "zonepath" : "/zones/freebsd"
}
